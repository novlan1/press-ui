{
  "name": "press-ui-next",
  "version": "0.0.17",
  "description": "简单、易用的跨端组件库，兼容 Vue2 和 Vue3，同时支持 uni-app和普通 Vue 项目",
  "keywords": [
    "press-ui",
    "uni-app",
    "uniapp",
    "component"
  ],
  "homepage": "https://novlan1.github.io/press-ui",
  "bugs": {
    "url": "https://github.com/novlan1/press-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/novlan1/press-ui"
  },
  "license": "MIT",
  "author": "guowangyang",
  "main": "cjs/index-lib.js",
  "jsdelivr": "dist/press-ui.min.js",
  "unpkg": "dist/press-ui.min.js",
  "module": "es/index.mjs",
  "typings": "es/index.d.ts",
  "files": [
    "es",
    "esm",
    "lib",
    "cjs",
    "dist",
    "typings",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "helper",
    "global.d.ts"
  ],
  "scripts": {
    "prebuild": "rimraf es/* esm/* lib/* cjs/* dist/* node_modules/press-ui/* --glob",
    "build": "cross-env NODE_ENV=production rollup -c script/build/rollup.config.ts && npm run build:tsc && npm run dts:update && npm run css:update",
    "init": "node script/build/init",
    "build:tsc": "concurrently \"npm:build:tsc-*\"",
    "build:tsc-es": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json --outDir es/",
    "build:tsc-esm": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json --outDir esm/",
    "build:tsc-lib": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json --outDir lib/",
    "build:tsc-cjs": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json --outDir cjs/",
    "dts:update": "node script/build/dts",
    "css:update": "node script/build/css-extra",
    "lint": "eslint --ext .js,.ts,.vue .",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue .",
    "lint:css": "stylelint **/*.{html,vue,css,sass,scss,less}",
    "lint:css:fix": "stylelint --fix **/*.{html,vue,css,sass,scss,less}"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.5",
    "press-ui": "^2.2.34",
    "tim-js-sdk": "^2.27.6"
  },
  "devDependencies": {
    "@babel/core": ">=7.13.0 <8.0.0",
    "@babel/helper-module-imports": "7.22.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.16.0",
    "@dcloudio/types": "^3.4.8",
    "@plugin-light/vite-plugin-alias-for-library": "^0.0.7",
    "@plugin-light/vite-plugin-gen-version": "^0.0.7",
    "@plugin-light/vite-plugin-ifdef": "^0.0.7",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-url": "^6.0.0",
    "@rushstack/eslint-patch": "^1.8.0",
    "t-comm": "^1.3.149",
    "@plugin-light/vite-plugin-ifdef": "^0.0.7",
    "@tsconfig/node20": "^20.1.4",
    "@types/babel__core": "^7.20.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/babel-plugin-jsx": "^1.2.5",
    "@vue/tsconfig": "^0.5.1",
    "@zebra-ui/swiper": "^2.2.8",
    "aegis-mp-sdk": "^1.39.1",
    "babel-plugin-component": "^1.1.1",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-light-vue3": "^0.1.5",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-plugin-vue": "^9.23.0",
    "glob": "^10.4.3",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.5.3",
    "rimraf": "^6.0.1",
    "rollup": "^2.47.0",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-esbuild": "^4.5.0",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-plugin-lit-css": "^5.0.2",
    "rollup-plugin-multi-input": "1.1.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-rename-node-modules": "^1.3.1",
    "rollup-plugin-static-import": "^0.1.1",
    "rollup-plugin-styles": "^3.14.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "rollup-pluginutils": "^2.8.2",
    "sass": "^1.77.6",
    "typescript": "~5.6.2",
    "vite": "^5.3.1",
    "vite-plugin-babel": "^1.2.0",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vitest": "^3.0.7",
    "vue-tsc": "^2.0.21"
  },
  "peerDependencies": {
    "vue": "^3.2.6"
  },
  "title": "press-ui"
}